Date=20-- Expiry date
Month=9 -- Expiry month 
Year=2020     -- Expiry year
expiremessage="🛑sᴄʀɪᴘᴛ sᴜ̈ʀᴇsɪ ᴅᴏʟᴍᴜşᴛᴜʀ...! ➕ɢᴜ̈ɴᴄᴇʟ sᴄʀɪᴘᴛɪ ᴛᴇʟᴇɢʀᴀᴍ ᴀᴅʀᴇsɪᴍᴅᴇɴ ɪɴᴅɪʀ...\n 💊ᴛ.ᴍᴇ/ᴀʀᴀᴘʙᴜᴋᴇɴɢᴇɴᴄ" 

function check(t) 
if t<10 then t="0"..t end
return t
end
expiredate=Year.. check(Month).. check(Date)
date=gg.makeRequest("https://whatismyipaddress.com").headers["Date" ][1]
month={"Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"}
for i=1,12 do
 if month[i]==string.sub(date,9, 11) then
      if i<10 then
       i="0"..i 
      end 
 currentdate=string.sub(date,13, 16)..i..string.sub(date,6, 7)  
 end
end
if tonumber(currentdate)>=tonumber(expiredate) then gg.alert(expiremessage) os.exit() end 
